{
    "presets": [
        [
            "env",
            {
                "targets": { // 配支持的环境
                    "browsers": [ // 浏览器
                        "last 2 versions",
                        "safari >= 7"
                    ],
                    "node": "current"
                },
                "modules": true, //设置ES6 模块转译的模块格式 默认是 commonjs
                "debug": true, // debug，编译的时候 console
                "useBuiltIns": false, // 是否开启自动支持 polyfill
                "include": [], // 总是启用哪些 plugins
                "exclude": [] // 强制不启用哪些 plugins，用来防止某些插件被启用
            }
        ]
    ]
}